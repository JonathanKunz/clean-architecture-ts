{
  "name": "interview",
  "version": "1.0.0",
  "description": "Interview Compasso UOL",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only -- src/main/server.ts",
    "prod": "npm run build && node dist/src/main/server.js",
    "dev": "node dist/src/main/server.js",
    "build": "rm -rf dist && npx tsc -p tsconfig-build.json",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test-verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --silent=false --watch -c jest-unit-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint": "npx prettier --check ./src",
    "format": "prettier --write ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonathanKunz/interview.git"
  },
  "author": "Jonathan MÃ¼ller Kunz",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "npm": "^6.14.8",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
